package com.lessrain.project.controller {	import com.lessrain.project.model.ToolsProxy;	import org.puremvc.as3.multicore.interfaces.INotification;	import org.puremvc.as3.multicore.patterns.command.SimpleCommand;	import flash.utils.getQualifiedClassName;	/**	 * @author Thomas Meyer, Less Rain (thomas@lessrain.com)	 */	public class ToolsLoadCommand extends SimpleCommand	{		public static const BODY_TYPE : Class = String;		override public function execute(notification_ : INotification) : void		{			if (BODY_TYPE && !(notification_.getBody() is BODY_TYPE))			{				throw new ArgumentError("Invalid notification body in " + getQualifiedClassName(this) + ". Expected " + BODY_TYPE + ", received " + ( notification_.getBody() ? getQualifiedClassName(notification_.getBody()) : "null" ));			}			var toolsProxy : ToolsProxy = ToolsProxy(facade.retrieveProxy(ToolsProxy.NAME));			toolsProxy.src = notification_.getBody() as String;			toolsProxy.load();		}	}}